{
	"name": "mireks",
	"version": "1.0.0",
	"description": "Nuxt Vue website - universal but still SPA.",
	"author": "Jakub Koralewski",
	"private": true,
	"scripts": {
		"dev": "nuxt",
		"build": "nuxt build",
		"start": "nuxt start",
		"generate": "nuxt generate",
		"test": "ava"
	},
	"ava": {
		"compileEnhancements": false,
		"extensions": [
			"ts"
		],
		"files": [
			"**/*.test.ts"
		],
		"sources": [
			"src/**/*"
		],
		"require": [
			"esm",
			"ts-node/register"
		],
		"verbose": true
	},
	"babel": {
		"presets": [
			"es2015"
		]
	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^1.2.19",
		"@fortawesome/free-regular-svg-icons": "^5.9.0",
		"@fortawesome/free-solid-svg-icons": "^5.9.0",
		"@fortawesome/vue-fontawesome": "^0.1.6",
		"@nuxtjs/sitemap": "^1.2.0",
		"@nuxtjs/style-resources": "^0.1.2",
		"@types/dom-inputevent": "^1.0.5",
		"@types/gsap": "^1.20.2",
		"cross-env": "^5.2.0",
		"d3plus-text": "^0.9.43",
		"gsap": "^2.1.3",
		"node-sass": "^4.12.0",
		"nuxt": "^2.0.0",
		"sass-loader": "^7.1.0",
		"scrollmagic": "^2.0.7",
		"ts-node": "^8.2.0",
		"vue-class-component": "^7.1.0"
	},
	"devDependencies": {
		"@nuxt/typescript": "^2.8.1",
		"ava": "^2.1.0",
		"babel-preset-env": "^1.7.0",
		"babel-register": "^6.26.0",
		"esm": "^3.2.25",
		"imports-loader": "^0.8.0",
		"jsdom": "^15.1.1",
		"nodemon": "^1.11.0",
		"nuxt-property-decorator": "^2.3.0",
		"postcss-loader": "^3.0.0",
		"prettier": "1.14.3",
		"prettier-tslint": "^0.4.2",
		"tslint": "^5.17.0",
		"tslint-config-prettier": "^1.18.0",
		"tslint-plugin-prettier": "^2.0.1",
		"vue-svg-inline-loader": "^1.2.15"
	}
}
